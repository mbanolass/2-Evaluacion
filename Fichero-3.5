#include<iostream>//Librería de cabecera
//Instrucciones básicas
//ejemplo de for: tabla de multiplicar
using namespace std;

int main(){
	int cont;
	int n_tabla;
	cout<<"Dime que tabla deseas: ";
	cin>>n_tabla;
	cout<<"************************"<<endl;
	cout<<"*      Tabla del  "<<n_tabla<<"     *"<<endl;
	cout<<"************************"<<endl;
	for(cont=0;cont<=10;cont++){
		cout<<n_tabla<<" x "<<cont<<" = "<<n_tabla*cont<<endl;
	}
	return 0;
	
}
